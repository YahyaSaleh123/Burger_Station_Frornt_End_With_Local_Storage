{
  "expo": {
    "name": "Burger Station",
    "slug": "Burger Station",
    "version": "1.0.0",
   
    "orientation": "portrait",
    "icon": "./assets/images/app_icon2.png",
    "scheme": "Burger Station",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },"extra": {
  "eas": {
    "projectId": "b611faf1-a0c1-4476-9d53-4c9d156fe286"
  }
}
   
  }
}
